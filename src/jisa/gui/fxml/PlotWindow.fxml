<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<BorderPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">


    <center>
        <AnchorPane fx:id="stack" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER">

        </AnchorPane>
    </center>
    <top>
        <ToolBar fx:id="toolbar" BorderPane.alignment="CENTER">
        </ToolBar>
    </top>

    <bottom>
        <HBox fx:id="sliderBox" alignment="CENTER" spacing="5" style="-fx-padding: 5 5 5 5; -fx-background-color: white;">
            <Label HBox.hgrow="NEVER" text="Range:"/>
            <Slider HBox.hgrow="ALWAYS" min="0.01" max="100.0" fx:id="rangeSliderX"/>
        </HBox>
    </bottom>

</BorderPane>
